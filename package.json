{
  "name": "solid-helper-vue",
  "version": "0.1.1",
  "description": "A helper library for developing SoLiD (Social Linked Data) under Vuejs",
  "main": "./dist/index.umd.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.umd.js"
      }
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "vite build",
    "dev": "vite",
    "serve": "vite preview"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/renyuneyun/solid-helper-vue.git"
  },
  "keywords": [
    "SoLiD",
    "vue",
    "vue3"
  ],
  "author": "Rui Zhao (renyuneyun)",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/renyuneyun/solid-helper-vue/issues"
  },
  "homepage": "https://github.com/renyuneyun/solid-helper-vue#readme",
  "dependencies": {
    "@inrupt/solid-client-authn-browser": "^1.16.0",
    "pinia": "^2.1.4",
    "vue": "^3.3.4"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.1.0",
    "typescript": "^5.0.2",
    "vite": "^4.3.9",
    "vite-plugin-dts": "^3.0.0-beta.1",
    "vue-tsc": "^1.4.2"
  }
}
